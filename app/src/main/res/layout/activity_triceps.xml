<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".triceps"
    android:background="@drawable/bg21">

    <Button
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:text="diamond pushups"
        android:textSize="25sp"
        android:drawableRight="@drawable/ic_action_play3"
        android:id="@+id/tr1"
        android:padding="0dp"
        android:layout_marginTop="70dp"
        android:textStyle="bold|italic"
        android:background="@drawable/btshape"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"/>
    <Button
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:text="bench dips"
        android:textSize="25sp"
        android:id="@+id/tr2"
        android:drawableRight="@drawable/ic_action_play3"
        android:layout_below="@+id/tr1"
        android:textStyle="bold|italic"
        android:layout_marginTop="10dp"
        android:background="@drawable/btshape"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"/>
    <Button
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:text="close-grip bench press"
        android:textSize="25sp"
        android:id="@+id/tr3"
        android:layout_below="@+id/tr2"
        android:layout_marginTop="10dp"
        android:drawableRight="@drawable/ic_action_play3"
        android:textStyle="bold|italic"
        android:background="@drawable/btshape"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"/>
    <Button
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:text="rope push down"
        android:textSize="25sp"
        android:id="@+id/tr4"
        android:layout_below="@+id/tr3"
        android:layout_marginTop="10dp"
        android:drawableRight="@drawable/ic_action_play3"
        android:textStyle="bold|italic"
        android:background="@drawable/btshape"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"/>
    <Button
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:text="skull crushes"
        android:textSize="25sp"
        android:id="@+id/tr5"
        android:drawableRight="@drawable/ic_action_play3"
        android:layout_below="@+id/tr4"
        android:layout_marginTop="10dp"
        android:textStyle="bold|italic"
        android:background="@drawable/btshape"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"/>
    <Button
        android:layout_width="400dp"
        android:layout_height="70dp"
        android:text="both arm dumbell extension"
        android:drawableRight="@drawable/ic_action_play3"
        android:textStyle="bold|italic"
        android:textSize="25sp"
        android:id="@+id/tr6"
        android:layout_below="@+id/tr5"
        android:layout_marginTop="10dp"
        android:background="@drawable/btshape"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"/>


</RelativeLayout>